        "target": "ESNext",
        "module": "commonjs",
        "rootDir": "./",
        "moduleResolution": "node",
        "baseUrl": "./src/",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
